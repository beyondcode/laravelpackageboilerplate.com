{
    "name": ":vendor/:package_name",
    "description": ":package_description",
    "keywords": [
        ":vendor",
        ":package_name"
    ],
    "homepage": "https://github.com/:vendor/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            ":vendor_namespace_escaped\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor_namespace_escaped\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                ":vendor_namespace_escaped\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": ":vendor_namespace\\Skeleton\\SkeletonFacade"
            }
        }
    }
}
